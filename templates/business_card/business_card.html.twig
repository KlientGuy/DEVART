    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <link rel="icon" type="image/svg+xml" href="{{ asset('favicon.svg') }}">
    <meta charset="UTF-8" />
    <style>
        body {
            width: 100%;
            height: 100%;
            margin: 0;
            font-family: 'Poppins';
            /*scroll-behavior: smooth;*/
        }
        html, body {
            overflow-x: hidden;
        }
        a {
            color: inherit;
            text-decoration: none;
        }
        .header {
            height: 40vh;
            background: #000000;
            background: linear-gradient(160deg, rgba(0, 0, 0, 1) 4%, rgba(0, 74, 212, 1) 100%);
            color: white;
            font-size: 3rem;
            font-weight: 500;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            gap: 7vh;
            position: relative;
        }
        .business-body{
            background-color: #FFFFFF;
            position: relative;
        }
        .business-offer {
            padding: 8vh;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            font-size: 2.5rem;
            font-weight: 500;
            gap: 2vh;
            color: #555555;
        }
        .contact-element {
            display: flex;
            align-items: center;
            gap: 3vw;
            width: 100vw;
            height: 10vh;
            margin-bottom: 20px;
            font-size: 2.5rem;
            margin-left: 10vw;
            padding: 1vw;
        }

        .icon-circle {
            width: 130px;
            height: 130px;
            background-color: #EEEEEE;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 50%;
            box-shadow: 0 2px 6px rgba(0,0,0,0.04);
        }

        .contact-bubbles {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 6vw;
            position: absolute;
            z-index: 2;
            top: 35vh;
            left: 50%;
            transform: translateX(-50%);
        }

        .icon-bubble {
            width: 150px;
            height: 150px;
            background-color: #ffffff;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 50%;
            -webkit-box-shadow: 4px 4px 10px 0px rgba(0, 0, 0, 0.25);
            -moz-box-shadow: 4px 4px 10px 0px rgba(0, 0, 0, 0.25);
            box-shadow: 4px 4px 10px 0px rgba(0, 0, 0, 0.25);
        }

        .contact-icon {
            width: 70px;
            height: 70px;
            fill: red;
        }

        .contact-info {
            display: flex;
            flex-direction: column;
            justify-content: center;
        }

        .contact-title {
            color: #555555;
            margin-bottom: 2px;
            font-size: 1.06em;
        }

        .contact-value {
            color: black;
            font-size: 0.98em;
            font-weight: 600;
        }

        .socials {
            display: flex;
            flex-direction: row;
            justify-content: center;
            align-items: center;
            gap: 10vw;
            margin-top: 7vh;
            margin-bottom: 4vh;
        }

    </style>
</head>
